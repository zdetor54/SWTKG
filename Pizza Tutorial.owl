                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/zacharias.detorakis/ontologies/2021/2/PizzaTutorial#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/zacharias.detorakis/ontologies/2021/2/PizzaTutorial#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella and Tomato toppings</rdfs:comment>
