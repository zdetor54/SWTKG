                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/zacharias.detorakis/ontologies/2021/2/PizzaTutorial#hasTopping"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/zacharias.detorakis/ontologies/2021/2/PizzaTutorial#PizzaTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
